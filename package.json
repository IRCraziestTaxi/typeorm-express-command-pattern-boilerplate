{
  "name": "typeorm-express-command-pattern-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate example of simple Express API using TypeORM and command pattern.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dist": "tsc",
    "mig:build": "tsc --project tsconfig.migrations-only.json",
    "mig:make": "typeorm migration:generate",
    "mig:revert": "typeorm migration:revert",
    "mig:run": "typeorm migration:run",
    "start": "node build/index",
    "tsc": "tsc --project tsconfig.local.json",
    "tslint": "tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IRCraziestTaxi/typeorm-express-command-pattern-boilerplate.git"
  },
  "keywords": [
    "typeorm",
    "express",
    "command",
    "pattern"
  ],
  "author": "IRCraziestTaxi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IRCraziestTaxi/typeorm-express-command-pattern-boilerplate/issues"
  },
  "homepage": "https://github.com/IRCraziestTaxi/typeorm-express-command-pattern-boilerplate#readme",
  "dependencies": {
    "@responsekit/core": "1.0.0",
    "@responsekit/express": "1.0.0",
    "@responsekit/tsmediator": "1.0.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.10.1",
    "cls-hooked": "4.2.2",
    "express": "4.17.3",
    "mysql": "2.17.1",
    "routing-controllers": "0.8.0",
    "ts-simple-automapper": "1.0.2",
    "ts-simple-nameof": "1.2.0",
    "tsmediator": "0.1.3",
    "typeorm": "0.2.19",
    "typeorm-linq-repository": "1.0.1",
    "typeorm-transactional-cls-hooked": "0.1.8"
  },
  "devDependencies": {
    "@types/cls-hooked": "4.3.0",
    "@types/express": "4.17.1",
    "tslint": "5.20.0",
    "typescript": "3.6.4"
  }
}
