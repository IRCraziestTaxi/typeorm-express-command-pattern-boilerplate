{
  "name": "typeorm-express-command-pattern-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate example of simple Express API using TypeORM and command pattern.",
  "main": "index.js",
  "scripts": {
    "mig:build": "node_modules/.bin/tsc --project tsconfig.migrations-only.json",
    "mig:make": "node_modules/.bin/typeorm migration:generate",
    "mig:revert": "node_modules/.bin/typeorm migration:revert",
    "mig:run": "node_modules/.bin/typeorm migration:run",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "node_modules/.bin/tsc",
    "tslint": "node_modules/.bin/tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IRCraziestTaxi/typeorm-express-command-pattern-boilerplate.git"
  },
  "keywords": [
    "typeorm",
    "express",
    "command",
    "pattern"
  ],
  "author": "IRCraziestTaxi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IRCraziestTaxi/typeorm-express-command-pattern-boilerplate/issues"
  },
  "homepage": "https://github.com/IRCraziestTaxi/typeorm-express-command-pattern-boilerplate#readme",
  "dependencies": {
    "cls-hooked": "4.2.2",
    "express": "4.17.1",
    "routing-controllers": "0.7.7",
    "ts-simple-automapper": "1.0.2",
    "ts-simple-nameof": "1.2.0",
    "tsmediator": "0.1.3",
    "typeorm": "0.2.19",
    "typeorm-linq-repository": "1.0.0",
    "typeorm-transactional-cls-hooked": "0.1.8"
  },
  "devDependencies": {
    "@types/cls-hooked": "4.3.0",
    "@types/express": "4.17.1",
    "tslint": "5.20.0",
    "typescript": "3.6.3"
  }
}
